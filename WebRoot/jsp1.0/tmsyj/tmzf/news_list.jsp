        function GetList(index,size) {
            $.ajax({
                url: "control/news_control.ashx",
                type: "post",
                dataType: 'json',
                data: { op: 'list', size: size, index: index,code:getUrlParam('code'), datatime: new Date() },
                success: function (msg) {
                    options.totalPages = Math.ceil(msg.total / 10);
                    options.currentPage = index;
                    $('#content').empty();
                    $.each(msg.data, function (index, item) {
                        var html = "";
                        if (index != msg.data.length - 1) {
                            html += "<li style=\"border-bottom:1px #999 dotted;height:135px;\">";
                        } else {
                            html += "<li style=\"border-bottom:1px #00a0ea solid;height:135px;\">";
                        }
                        html += " <table width=\"100%\"  style=\"text-align:left;margin-top:15px;\">";
                        html += "  <tr>";
                        html += "      <td rowspan=\"3\" width=\"140px\"  style=\"padding-right:15px;\">";
                        html += "         <a href=\"news_info.html?id=" + item.id + "\" target='_blank'><img src=\"" + (item.imgurl == "" ? "images/kong.jpg" : item.imgurl) + "\" style=\"height:120px;width:160px;\" /></a>";
                        html += "     </td>";
                        html += "    <td  height=\"30px\"><a href=\"news_info.html?id=" + item.id + "\" target='_blank' style=\"font-size:17px;color:#777779;text-decoration:none;font-weight:bold;;\" >" + item.title + "</a></td>";
                        html += "</tr>";
                        html += " <tr>";
                        html += "   <td  style=\"font-size:14px;vertical-align:top;\">" + item.summary + "</td>";
                        html += " </tr>";
                        html += " <tr>";
                        html += "   <td  style=\"font-size:14px;vertical-align:top;\">发布时间：" + item.upload_time + "</td>";
                        html += " </tr>";
                        html += "</table>";
                        html += "</li>";
                        $('#content').append(html);
                        if (index == msg.data.length - 1) {
                            $('#page_list').bootstrapPaginator(options);
                        }
                    });

                }
            });
        }
