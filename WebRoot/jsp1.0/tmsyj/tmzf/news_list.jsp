    $(function () {
        $('#top').load('template/top.html');
        $('#foot').load('template/foot.html');
        $('#pop').load('template/news.html');
        if (getUrlParam('code') != "") {
            $.ajax({
                url: "control/get_type_control.ashx",
                type: "post",
                dataType: 'json',
                data: { op: 'codes', ecode: getUrlParam('code'), datatime: new Date() },
                success: function (msg) {
                    if (msg[0].name == msg[1].name) {
                        $('#top_title').html('当前位置：' + msg[0].name);
